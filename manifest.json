{
  "manifest_version": 3,
  "name": "Color Picker Extension",
  "version": "1.0",
  "description": "A simple color picker extension",
  "permissions": [
    {
      "host_permissions": [
        "http://*/*",
        "https://*/*",
        "http://localhost/*",
        "https://localhost/*"
      ]
    },
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
